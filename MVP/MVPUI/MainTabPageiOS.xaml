<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Microsoft.Mvpui.MainTabPageiOS"
             xmlns:local="clr-namespace:Microsoft.Mvpui;assembly=Mvpui">
    <local:MVPNavigationPage Icon="tab_profile.png" Title="My Profile">
        <x:Arguments>
            <local:MyProfile/>
        </x:Arguments>
    </local:MVPNavigationPage>
    <local:MVPNavigationPage Icon="tab_contribution.png" Title="Contributions">
        <x:Arguments>
            <local:ContributionsPage/>
        </x:Arguments>
    </local:MVPNavigationPage>
</TabbedPage>